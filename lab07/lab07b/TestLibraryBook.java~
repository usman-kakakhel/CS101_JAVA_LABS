   */
  public void books() 
  {
    //variables
    
    
    //program code
    
    book1 = new LibraryBook("kite runer", "khaled hossseni", "29-11-17", 5);
    book2 = new LibraryBook("angels and demons", "Dan brown", "29-11-17", 10);
    System.out.println(book1);
    System.out.println(book2);
    
    book1.loanBook("4-12-17");
    book2.loanBook("5-12-17");
   System.out.println(book1);
    System.out.println(book2);
    
    book1.returnBook();
    book2.returnBook();
   System.out.println(book1);
    System.out.println(book2);
    
